{
  "name": "x-react-cli",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8008 BROWSER=none react-app-rewired start",
    "build": "cross-env BUILD_ENV=production react-app-rewired build",
    "build:analyzer": "cross-env ANALYZER=true react-app-rewired build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "eslint-config-prettier": "^8.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "node-sass": "npm:dart-sass",
    "prettier": "2.5.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-app-rewired": "^2.2.1",
    "terser-webpack-plugin": "^5.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
